            pos: self.pos
            size: self.size
            source: 'wea2.jpg'
    Label:
        font_size: 150
        text:"Lets Check the Weather"
        font_name:"Quake"
        color: 1,0,1,1
    Label:
        color:1,0,0,1
        font_size: 35
        text:'Enter City Name'
